	<title>SPOT</title>
	<meta charset="UFT-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta charset="UFT-8">
	<link href="./css/bootstrap.min.css" rel="stylesheet">
	<link href="./css/bootstrap-theme.min.css" rel="stylesheet">
	<link rel="stylesheet" href="./css/spot.css">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">



	
</head>
<body>
	<div class="pagetitle">
		<h2>Result</h2>
	</div>
	<div class="container">
		<div class="tip"><i class="fa fa-question-circle"></i>
	  		<span class="tiptext">Here are some recommendations for you! Select No to see another option, select Yes to open navigation or select View to see more details :)

	  			You can tap/drag the bubbles to select the one correspond with your current state
	  		</span>
	  	</div>
		

		{{#if output.Alt}}
			{{#if output.logged}}
		  		{{#each output}}
					<a href="./place/{{placeId}}" onclick="ga('create', 'UA-114618844-1','auto');ga('send', 'event', 'PlaceAlt', 'click')">
						<div style="background: #f6f6f6; " class="thumbnail">
							<h5>{{name}}</h5>
							<img style="width: 50%;display: inline-block;"" src="{{imageURL}}" class="img-responsive">
							
								<p style="display: inline-block; margin:0 auto">{{rate}}</p>
							
						</div>
					</a>
				{{/each}}
			{{else}}
				{{#each output}}
					<a href="./placeNL/{{placeId}}" onclick="ga('create', 'UA-114618844-1','auto');ga('send', 'event', 'PlaceAlt', 'click')">
						<div class="thumbnail">
							<h5>{{name}}</h5>
							<img src="{{imageURL}}" class="img-responsive">
							
								<h4 style="text-align: center;">{{rate}}</h4>
							
						</div>
					</a>
				{{/each}}
			{{/if}}
				

		{{else}}
			<h4>{{output.[0].name}}</h4>
			<img src="{{output.[0].imageURL}}" class="img-responsive">
			<div class="info">
				<h4 style="text-align: center; ">{{output.[0].rate}}</h4>
			</div>
			{{#if output.logged}}
				<div class="search-options">
					<a href="./result_next" onclick="ga('create', 'UA-114618844-1','auto');ga('send', 'event', 'No', 'click')"><button type="button">No</button></a>
					<a href="./placeNL/{{output.[0].placeId}}" onclick="ga('create', 'UA-114618844-1','auto');if (window.performance) {
  																// Gets the number of milliseconds since page load
  																// (and rounds the result since the value must be an integer).
  																var timeSincePageLoad = Math.round(performance.now());

  																// Sends the timing hit to Google Analytics.
  									ga('send', 'timing', 'timeDiff', 'resultPage', timeSincePageLoad);}"><button type="button">View</button></a>
					<a href="./navigation"><button id= "GoB" type="button" onclick="ga('create', 'UA-114618844-1','auto');ga('send', 'event', 'Go', 'click')">Go!</button></a>
				</div>
				<br>
				<input type="button" value="Back" onclick="history.back()">

			{{else}}
				<div style="vertical-align: center;" class="search-options">
					<a href="./resultNL_next" onclick="ga('create', 'UA-114618844-1','auto');ga('send', 'event', 'No', 'click')"><button type="button">No</button></a>
					<a href="./placeNL/{{output.[0].placeId}}" onclick="ga('create', 'UA-114618844-1','auto');if (window.performance) {
  																// Gets the number of milliseconds since page load
  																// (and rounds the result since the value must be an integer).
  																var timeSincePageLoad = Math.round(performance.now());

  																// Sends the timing hit to Google Analytics.
  									ga('send', 'timing', 'timeDiff', 'resultPage', timeSincePageLoad);}"><button type="button">View</button></a>
					<a href="./navigationNL"><button type="button"  onclick="ga('create', 'UA-114618844-1','auto');ga('send', 'event', 'Go', 'click')">Go!</button></a>
				</div>
				<br>
				<input type="button" value="Back" onclick="history.back()">
				

			{{/if}}
		{{/if}}	
		
	</div>

	
		{{#if output.logged}}
				
				<div class="icon-bar">
					<a href="./"><i class="fa fa-home"></i></a> 
					<a href="./search"><i class="fa fa-search"></i></a>  
					<a href="./favorites"><i class="fa fa-heart"></i></a>
					<a href="./profile"><i class="fa fa-user"></i></a> 
				</div>
		{{else}}
				
				<div class="icon-bar">
					<a href="./"><i class="fa fa-home"></i></a> 
					<a href="./searchNL"><i class="fa fa-search"></i></a>  
					<a href="./login"><i class="fa fa-heart"></i></a>
					<a href="./login"><i class="fa fa-user"></i></a> 
				</div>
				
		{{/if}}
		
	
